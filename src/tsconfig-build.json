
{
  "compilerOptions": {
    "declaration": true,
    "stripInternal": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "target": "es6",
    "skipLibCheck": true,
    "types": [],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "sourceMap": true,
    "inlineSources": true,
    "importHelpers": true
  }
}